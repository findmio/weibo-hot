{
    "compilerOptions": {
        "module": "ES2015",
        "declaration": true,
        "strict": true,
        "removeComments": false,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "target": "ES2015",
        "sourceMap": false,
        "skipLibCheck": true,
        "useUnknownInCatchVariables": false,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node"
    },
    "include": [
        "main.ts"
    ]
}